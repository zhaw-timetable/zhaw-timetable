{
  "name": "zhaw-timetable-backend",
  "version": "0.0.1",
  "description": "A timetable progressive web application for ZHAW School of Engineering",
  "main": "main.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon -w app --exec babel-node app",
    "build": "./node_modules/.bin/babel app -d dist --copy-files --presets env,es2015,stage-2",
    "serve": "node dist",
    "test": "jest --coverage",
    "test-only": "jest",
    "watch": "jest --watch",
    "codecov": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaw-timetable/zhawo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhaw-timetable/zhawo/issues"
  },
  "homepage": "https://github.com/zhaw-timetable/zhawo#readme",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "date-fns": "^1.29.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.1",
    "morgan": "^1.9.1",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.7",
    "rss-parser": "^3.5.4",
    "signale": "^1.3.0"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "jest": "^24.1.0",
    "jest-fetch-mock": "^1.6.6",
    "superagent": "^4.0.0-beta.5",
    "supertest": "^3.3.0"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./app/setupJest.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/app/db.js",
      "<rootDir>/app/logger.js",
      "<rootDir>/app/setupJest.js"
    ]
  }
}
